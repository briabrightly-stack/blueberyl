{
  "version": 2,
  "headers": [
    {
      "source": "/public/images/_tree-diagram/manifest.json",
      "headers": [
        { "key": "Content-Type", "value": "application/ld+json; charset=utf-8" }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Access-Control-Allow-Methods", "value": "GET, OPTIONS" },
        { "key": "Access-Control-Allow-Headers", "value": "Origin, Accept, Content-Type" }
      ]
    }
  ]
}
